package hola;

import java.awt.Graphics2D;
import java.awt.image.BufferedImage;

public class Main {
    public static void main(String[] args) {
        BufferedImage image = new BufferedImage(PixelArtUno.width, PixelArtUno.height, BufferedImage.TYPE_INT_RGB);
        Graphics2D g = image.createGraphics();
        PixelArtUno.paint(g, image, PixelArtUno.width,PixelArtUno.height );
        
        BufferedImage image2 = new BufferedImage(PixelArtDos.width2, PixelArtDos.height2, BufferedImage.TYPE_INT_RGB);
        Graphics2D g2 = image2.createGraphics();
        PixelArtDos.paint2(g2, image2, PixelArtDos.width2,PixelArtDos.height2 );
    }
}
